{

    "name": "subscription/service",
    "description": "laravel payment service",
    "type": "metapackage",
    "autoload": {
        "psr-4": {
            "SubscriptionService\\": "subscriptionService/src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SubscriptionService\\SubscriptionServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "SF",
            "email": "p.manteali@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2.0",
        "illuminate/database": ">=5.0",
        "illuminate/support": ">=5.0"
    }
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
